{
  "name": "snocream2",
  "version": "1.0.0",
  "description": "This is a remake of Snocream's website",
  "main": "app.js",
  "scripts": {
    "start": "node app",
    "start-dev": "nodemon app",
    "sass:watch-user": "node-sass -w ./sass/main.scss ./public/css/style.css --output-style='compressed'",
    "sass:watch-admin": "node-sass -w ./sass/adminMain.scss ./public/css/adminStyle.css --output-style='compressed'",
    "sass:watch": "concurrently \"npm run sass:watch-user\" \"npm run sass:watch-admin\""
  },
  "keywords": [
    "Snocream",
    "Knoxville"
  ],
  "author": "Hayden Bradfield",
  "license": "ISC",
  "dependencies": {
    "array-unique": "^0.3.2",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "ejs": "^3.0.1",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.6",
    "express-session": "^1.17.0",
    "helmet": "^3.22.0",
    "jimp": "^0.10.0",
    "knex": "^0.20.13",
    "method-override": "^3.0.0",
    "moment": "^2.24.0",
    "morgan": "^1.10.0",
    "sqlite3": "^4.1.1"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "node-sass": "^7.0.0",
    "nodemon": "^2.0.2"
  }
}
